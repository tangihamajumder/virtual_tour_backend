{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "warn",
    "no-unused-expressions": "error"
  },
  "globals": {
    "process": "readonly"
  }
}
